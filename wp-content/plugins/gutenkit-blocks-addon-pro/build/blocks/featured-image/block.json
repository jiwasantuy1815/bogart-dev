    "alignmentMobileLandscape": {
      "type": "string"
    },
    "alignmentLaptop": {
      "type": "string"
    },
    "alignmentWideScreen": {
      "type": "string"
    },
    "cssFilters": {
      "type": "object"
    },
    "cssFiltersHover": {
      "type": "object"
    },
    "borderHoverColor": {
      "type": "string"
    },
    "overlayBackgroundHover": {
      "type": "object"
    },
    "boxShadowHover": {
      "type": "object"
    },
    "boxShadow": {
      "type": "object"
    },
    "captionSource": {
      "type": "string",
      "default": "none"
    },
    "customCaption": {
      "type": "string"
    },
    "captionAlignmentDesktop": {
      "type": "string"
    },
    "captionAlignmentTablet": {
      "type": "string"
    },
    "captionAlignmentMobile": {
      "type": "string"
    },
    "captionAlignmentTabletLandscape": {
      "type": "string"
    },
    "captionAlignmentMobileLandscape": {
      "type": "string"
