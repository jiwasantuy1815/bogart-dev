    "borderRadiusTabletLandscape": {
      "type": "object"
    },
    "borderRadiusMobileLandscape": {
      "type": "object"
    },
    "borderRadiusLaptop": {
      "type": "object"
    },
    "borderRadiusWideScreen": {
      "type": "object"
    },
