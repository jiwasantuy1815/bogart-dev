      "type": "boolean",
      "default": false
    },
    "postNewTab": {
      "type": "boolean",
      "default": false
    },
    "postNoFollow": {
      "type": "boolean",
      "default": false
    }
  },
  "usesContext": [
    "postId",
    "postType",
    "queryId"
  ],
  "supports": {
    "html": false,
    "anchor": true
  },
  "textdomain": "gutenkit-blocks-addon-pro",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": [
    "file:./style-index.css",
    "fancybox"
  ],
  "render": "file:./render.php",
  "script": [
    "fancybox"
  ],
  "viewScript": "file:./frontend.js"
}