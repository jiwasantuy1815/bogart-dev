{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenkit-pro/post-title",
  "version": "1.0.0",
  "title": "Post Title",
  "category": "gutenkit",
  "keywords": [
    "gkit",
    "gutenkit",
    "gutenkit pro"
  ],
  "description": "Post Title block for Gutenkit Pro.",
  "ancestor": [
    "gutenkit-pro/query-template",
    "gutenkit-pro/advanced-search"
  ],
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "attributes": {
    "blockID": {
      "type": "string"
    },
    "blockClass": {
      "type": "string"
    },
    "blocksCSS": {
      "type": "object"
    },
    "gkitQueryPostTitleBefore": {
      "type": "string",
      "default": ""
    },
    "gkitQueryPostTitleAfter": {
      "type": "string",
      "default": ""
    },
    "gkitQueryPostTitleFallback": {
      "type": "string",
      "default": "Post Title"
    },
    "gkitQueryPostLink": {
      "type": "string",
      "default": "post"
    },
    "gkitQueryCustomPostTitleLink": {
      "type": "object"
    },
    "gkitQueryPostTitleTag": {
      "type": "string",
      "default": "h3"
    },
    "QueryPostTitleLength": {
      "type": "number",
      "default": 50
    },
    "gkitQueryStroke": {
      "type": "object"
    },
    "gkitQueryPostTitleAlignmentDesktop": {
      "type": "string",
      "default": "left"
    },
    "gkitQueryPostTitleAlignmentTablet": {
      "type": "string"
    },
    "gkitQueryPostTitleAlignmentMobile": {
      "type": "string"
    },
    "gkitQueryPostTitleAlignmentTabletLandscape": {
      "type": "string"
    },
    "gkitQueryPostTitleAlignmentMobileLandscape": {
      "type": "string"
    },
    "gkitQueryPostTitleAlignmentLaptop": {
      "type": "string"
    },
    "gkitQueryPostTitleAlignmentWideScreen": {
      "type": "string"
    },
    "gkitQueryPostTitleColor": {
      "type": "string"
    },
    "gkitQueryPostTitleColorHover": {
      "type": "string"
    },
    "gkitQueryPostTitleTypography": {
      "type": "object"
    }
  },
  "usesContext": [
    "postType",
    "postId"
  ],
  "supports": {
    "html": false,
    "anchor": true
  },
  "textdomain": "gutenkit-blocks-addon-pro",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}