{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenkit-pro/advanced-toggle",
  "version": "1.0.0",
  "title": "Advanced Toggle",
  "category": "gutenkit",
  "keywords": [
    "gkit",
    "toggle",
    "advanced",
    "advanced-toggle",
    "gutenkit-pro"
  ],
  "description": "Advanced Toggle for Gutenberg block.",
  "allowedBlocks": [
    "gutenkit-pro/advanced-toggle-item"
  ],
  "example": {
    "preview": true
  },
  "attributes": {
    "blockID": {
      "type": "string"
    },
    "blockClass": {
      "type": "string"
    },
    "blocksCSS": {
      "type": "object"
    },
    "style": {
      "type": "string",
      "default": "tab"
    },
    "gkitToggleItems": {
      "type": "array",
      "default": [
        {
          "toggleTitle": "Annual #1",
          "toggleColor": "#fffff",
          "toggleBackground": "#1868dd",
          "toggleActive": true
        },
        {
          "toggleTitle": "Monthly #2",
          "toggleColor": "#fffff",
          "toggleBackground": "#1868dd",
          "toggleActive": false
        }
      ],
      "excludeCopy": true
    },
    "togglePosition": {
      "type": "string",
      "default": "left"
    },
    "innerIndex": {
      "type": "number",
      "default": 0
    },
    "annualTitle": {
      "type": "string",
      "default": "Annual"
    },
    "monthlyTitle": {
      "type": "string",
      "default": "Monthly"
    },
    "tabsBackground": {
      "type": "object"
